<!DOCTYPE html>
<!-- Landing Page -->
<html lang="en" style="background-color: papayawhip;">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title> wassaapps </title>
    <script src="https://kit.fontawesome.com/1c86f335dd.js" crossorigin="anonymous"></script>
    <link rel='stylesheet' href="https://bootswatch.com/4/minty/bootstrap.min.css">
</head>

<body style="background-color: papayawhip;">
    <nav class="navbar navbar-expand-lg navbar-dark bg-primary">
        <a id="nav-wassaapps" class="navbar-brand" href="/"> Wassaapps </a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarColor01" aria-controls="navbarColor01" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="bg-secondary" style="border-left: 6px solid #F3969A; height: 40px;"></div>
        <div class="collapse navbar-collapse" id="navbarColor01">
          <ul class="navbar-nav mr-auto">
            <li id="nav-login" class="nav-item"><a class="nav-link" href="/login"> Login </a></li>
            <li id="nav-flash" class="nav-item"><a class="nav-link disabled"> Flashcards </a></li>
            <li id="nav-cab" class="nav-item"><a class="nav-link disabled"> Cabinets </a></li>
          </ul>
          <form class="form-inline my-2 my-lg-0" action="https://google.com/search" method="get" target="_blank">
            <button class="btn btn-secondary my-2 my-sm-0" type="submit"> Search </button>
            <input class="form-control ml-sm-2" type="text" name="q" placeholder="Google Search">
          </form>
          <div class="bg-secondary mx-2" style="border-left: 6px solid #F3969A; height: 40px;"></div>
          <form class="form-inline my-2 my-lg-0" action="https://youtube.com/search" method="get" target="_blank">
            <input class="form-control mr-sm-2" type="text" name="q" placeholder="Youtube Search">
            <button class="btn btn-secondary my-2 my-sm-0" type="submit"> Search </button>
          </form>
        </div>
    </nav>

    <%if(typeof background != 'undefined'){ %>
    <div id="row-mb-1" style="background-color: <%= background %>;"> <%- body %> <!-- Output views on that page with background color --> </div>
    <% } %>

    <%if(typeof background == 'undefined'){ %>
    <div id="row-mb-1"> <%- body %> <!-- Output views on that page--> </div>
    <% } %>

    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
</body>
</html>